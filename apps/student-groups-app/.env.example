# Canvas LMS Configuration
# Copy this file to .env and fill in your actual values

# Canvas API Access Token (get from Canvas Settings > Approved Integrations)
# This token will be exposed to the frontend via VITE_ prefix
VITE_CANVAS_ACCESS_TOKEN=your_canvas_access_token_here

# Backend API Base URL (default points to local backend proxy)
VITE_CANVAS_API_BASE=http://localhost:3001/api/canvas